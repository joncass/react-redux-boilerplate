{
  "name": "react-redux-boilerplate",
  "version": "0.0.1",
  "description": "My boilerplate / starter repo for react-redux projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/joncass/react-redux-boilerplate"
  },
  "scripts": {
    "start": "webpack-dev-server --env=development --progress --profile --colors",
    "build": "webpack --env=production --progress --profile --colors",
    "lint": "eslint ./src/app && echo ESLINT: Congratulations! You are lint-free!",
    "jest": "jest --verbose",
    "test": "npm run lint && npm run jest"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^20.0.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^20.0.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "webpack": "^2.5.1"
  }
}
